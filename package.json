{
  "name": "knab-james-frontend",
  "version": "0.0.3",
  "description": "Knab Verzekeren Web app (Codename James)",
  "author": "cXstudio",
  "scripts": {
    "commitmsg": "commitlint -e && npm run lint:styles && npm run lint",
    "prepush": "npm run test && npm run security",
    "config": "ts-node ./scripts/set-env.ts",
    "start": "npm run config -- --no-regenerate=true && ng serve --open --locale --aot",
    "build": "npm run config -- --analytics=true && ng build --locale --aot --build-optimizer",
    "build:prod": "npm run config -- --environment=prod --analytics=true && ng build --prod --environment=prod --locale --build-optimizer",
    "serve:prod": "ng serve --open --locale --prod",
    "serve:gtm": "npm run config -- --analytics=true && ng serve --open --locale",
    "test:watch": "ng test --code-coverage --watch --sourcemaps=true",
    "test": "npm run config && ng test --code-coverage --single-run --sourcemaps=false",
    "test:ci": "npm run config && ng test --code-coverage --watch=false",
    "security": "retire -npv --exitwith 0 --proxy http://edc-proxy.ds.global:9090",
    "lint": "ng lint --type-check",
    "lint:styles": "stylelint 'src/**/*.scss' --syntax scss --config stylelint.conf.js",
    "e2e": "ng e2e",
    "clean": "rimraf -- node_modules dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@knx/avatar": "0.9.31",
    "@knx/base": "2.42.77",
    "@knx/checkbox": "1.0.62",
    "@knx/collapsible-panel": "0.0.243",
    "@knx/currency-input": "2.0.76",
    "@knx/date-input": "1.0.59",
    "@knx/dropdown": "0.0.218",
    "@knx/form-control": "1.0.61",
    "@knx/form-group": "1.0.61",
    "@knx/forms": "1.0.61",
    "@knx/grid": "0.0.374",
    "@knx/input": "0.0.60",
    "@knx/loader": "1.0.58",
    "@knx/locale": "1.0.58",
    "@knx/message": "1.0.58",
    "@knx/modal": "0.0.81",
    "@knx/password-strength": "0.0.50",
    "@knx/postal-code-input": "3.0.52",
    "@knx/radio": "1.0.58",
    "@knx/select": "1.0.67",
    "@knx/step-navigation": "0.0.117",
    "@knx/textarea": "1.0.58",
    "@knx/tooltip": "1.0.87",
    "@knx/utils": "1.0.27",
    "@knx/wizard": "0.0.216",
    "@ngrx/effects": "^4.0.2",
    "@ngrx/router-store": "^4.0.2",
    "@ngrx/store": "^4.0.2",
    "@ngrx/store-devtools": "^4.0.0",
    "angular-svg-icon": "^4.2.0",
    "angular2-text-mask": "^8.0.4",
    "core-js": "^2.4.1",
    "cuid": "^1.3.8",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "lodash.clonedeep": "^4.5.0",
    "moment": "~2.18.1",
    "node-forge": "^0.7.1",
    "redux-beacon": "^1.1.0",
