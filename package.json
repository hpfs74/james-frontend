{
  "name": "knab-james-frontend",
  "version": "0.0.3",
  "description": "Knab Verzekeren Web app (Codename James)",
  "author": "cXstudio",
  "scripts": {
    "commitmsg": "conventional-changelog-lint -e && npm run lint:styles && npm run lint",
    "prepush": "npm run test && nsp check",
    "config": "ts-node ./scripts/set-env.ts",
    "start": "npm run config && ng serve --open --locale --extract-css",
    "build": "npm run config --analytics=true && ng build --aot --locale",
    "build:prod": "npm run config -- --environment=prod --analytics=true && ng build --prod --environment=prod --locale",
    "serve:prod": "ng serve --open --locale --prod",
    "serve:gtm": "npm run config -- --analytics=true && ng serve --open --locale --extract-css",
    "test": "npm run config && ng test --code-coverage --single-run --sourcemaps=false",
    "test:jest": "jest",
    "lint": "ng lint --type-check",
    "lint:styles": "stylelint 'src/**/*.scss' --syntax scss --config stylelint.conf.js",
    "e2e": "ng e2e",
    "clean": "npm cache clean && rimraf -- node_modules dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.5",
    "@angular/app-shell": "^0.1.0",
    "@angular/common": "^4.4.5",
    "@angular/compiler": "^4.4.5",
    "@angular/core": "^4.4.5",
    "@angular/forms": "^4.4.5",
    "@angular/http": "^4.4.5",
    "@angular/platform-browser": "^4.4.5",
    "@angular/platform-browser-dynamic": "^4.4.5",
    "@angular/router": "^4.4.5",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@cx/base": "2.6.1",
    "@cx/checkbox": "2.2.2",
    "@cx/currency-input": "2.0.3",
    "@cx/date-input": "2.2.2",
    "@cx/file-upload": "2.2.2",
    "@cx/form": "1.11.0",
    "@cx/form-control": "2.2.2",
    "@cx/form-group": "2.2.2",
    "@cx/forms": "2.2.3",
    "@cx/input": "2.1.3",
    "@cx/postal-code-input": "2.1.3",
    "@cx/radio": "2.1.3",
    "@cx/select": "2.2.1",
    "@cx/slider": "2.1.2",
    "@cx/textarea": "2.1.2",
    "@cx/utils": "1.4.0",
    "@knx/base": "^2.38.1",
    "@knx/collapsible-panel": "0.0.92",
    "@knx/grid": "0.0.278",
    "@knx/info": "0.0.132",
    "@knx/modal": "0.0.12",
    "@knx/step-navigation": "0.0.12",
    "@knx/wizard": "0.0.152",
    "@ngrx/effects": "^4.0.2",
    "@ngrx/router-store": "^4.0.2",
    "@ngrx/store": "^4.0.2",
    "@ngrx/store-devtools": "^4.0.0",
    "angular-svg-icon": "^4.2.0",
    "core-js": "^2.4.1",
    "cuid": "^1.3.8",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "lodash.clonedeep": "^4.5.0",
    "moment": "~2.18.1",
    "node-forge": "^0.7.1",
    "normalizr": "^3.2.4",
    "redux-beacon": "^1.1.0",
    "rxjs": "^5.4.1",
    "stacktrace-js": "^2.0.0",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.2.7",
    "@angular/compiler-cli": "^4.4.5",
    "@angular/language-service": "^4.4.5",
    "@angular/service-worker": "^1.0.0-beta.16",
    "@commitlint/config-angular": "^3.1.1",
    "@types/cuid": "^1.3.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^20.0.8",
    "@types/node": "^6.0.89",
    "chalk": "^2.0.1",
    "codelyzer": "~3.0.1",
    "conventional-changelog-lint": "^2.1.1",
    "dotenv": "^4.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.6.2",
    "jasmine-marbles": "^0.1.0",
    "jasmine-spec-reporter": "~4.1.0",
    "jest": "^21.1.0",
    "jest-junit": "^3.1.0",
    "jest-preset-angular": "^3.0.1",
    "karma": "^1.7.1",
    "karma-bamboo-reporter": "^0.1.2",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-remap-istanbul": "^0.6.0",
    "karma-sourcemap-loader": "^0.3.7",
    "ng-router-loader": "^2.1.0",
    "ngrx-store-freeze": "^0.1.9",
    "nsp": "^2.7.0",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.1",
    "stylelint": "~8.1.1",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "yargs": "^8.0.2"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupTestFrameworkScriptFile": "./setupJest.ts",
    "testResultsProcessor": "./node_modules/jest-junit",
    "transformIgnorePatterns": []
  }
}
